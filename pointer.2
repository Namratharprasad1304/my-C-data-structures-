#include <stdio.h>

int main() {
    int data[] = {10, 20, 30, 40, 50}; 
    int *p;                          
    int i;                          

    
    
    p = data;                        
                                     

    printf("Accessing Array Elements using Pointer:\n");
    
    
    for (i = 0; i < 5; i++) {
        
        printf("Element %d: %d\n", i, *p); 
        p++;                         
    }
    
    return 0;
}
